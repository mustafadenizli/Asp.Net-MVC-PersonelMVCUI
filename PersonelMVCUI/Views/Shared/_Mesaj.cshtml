@model PersonelMVCUI.ViewModels.MesajViewModel


@if (Model.Status)
{
    <div class="alert alert-success">
        @Model.Mesaj <a href="@Model.Url" class="btn btn-success">@Model.LinkTest</a>
    </div>
}
else
{
    <div class="alert alert-danger">
        @Model.Mesaj <a href="@Model.Url" class="btn btn-success">@Model.LinkTest</a>
    </div>
}
